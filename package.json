{
  "name": "@myoschen/eslint-config",
  "version": "1.0.8",
  "description": "My eslint configurations.",
  "license": "MIT",
  "author": "Myoschen <myos.chen@gmail.com> (https://github.com/Myoschen/)",
  "keywords": [
    "eslint-config"
  ],
  "homepage": "https://github.com/Myoschen/eslint-config#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Myoschen/eslint-config.git"
  },
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": "./dist/index.js"
  },
  "files": [
    "dist"
  ],
  "publishConfig": {
    "access": "public"
  },
  "type": "module",
  "scripts": {
    "dev": "tsx src/index.ts",
    "build": "tsup src/index.ts",
    "lint": "eslint --quiet",
    "lint:fix": "eslint --fix --quiet",
    "publish": "bumpp && pnpm publish",
    "prepublishOnly": "nr build"
  },
  "dependencies": {
    "@eslint-react/eslint-plugin": "^1.26.2",
    "@next/eslint-plugin-next": "^15.1.6",
    "@stylistic/eslint-plugin": "^3.1.0",
    "@typescript-eslint/eslint-plugin": "^8.23.0",
    "@typescript-eslint/parser": "^8.23.0",
    "eslint": "^9.20.0",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-react-hooks": "^5.1.0",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "eslint-plugin-tailwindcss": "^3.18.0",
    "globals": "^15.14.0"
  },
  "devDependencies": {
    "@types/eslint-plugin-jsx-a11y": "^6.10.0",
    "@types/eslint-plugin-tailwindcss": "^3.17.0",
    "@types/node": "^22.13.1",
    "bumpp": "^10.0.2",
    "jiti": "^2.4.2",
    "tsup": "^8.3.6",
    "tsx": "^4.19.2",
    "typescript": "^5.7.3"
  },
  "peerDependencies": {
    "eslint": ">= 9"
  }
}
